{
    "builds": [
      {
        "src": "command-center-backend/command_center/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "buildCommand": "bash build.sh"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "command-center-backend/command_center/wsgi.py"
      }
    ]
  }